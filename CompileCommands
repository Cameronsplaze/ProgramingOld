() = File Names
<> = Optional

Using C++ with SFML:
	g++ -c main.cpp
	g++ main.o -o (newFileName) -lsfml-graphics -lsfml-window -lsfml-system <-lsfml-audio> <-lsfml-network>
	./(newFileName)
Using C++
	g++ <-std=c++14> -o (newFileName) (File1.cpp)(File2.cpp)
	./(newFileName)
using Crystal
	crystal (newFile.cr)
Using Forth
	gforth (File1.fs)(File2.fs) -e bye
using Haskell
	ghc -o (newFileName) (File1.hs)(File2.hs)
	./(newFileName)
using Lua
	luac -o (newFileName) (File1.lua)(File2.lua)
	lua (newFileName)
using Python
	python3 (File1.py)

